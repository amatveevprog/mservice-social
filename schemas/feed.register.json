{
  "type": "object",
  "additionalProperties": false,
  "required": [ "internal", "network", "filter" ],
  "properties": {
    "internal": {
      "description": "Internal ID to assign this feed to",
      "type": "string"
    },
    "network": {
      "description": "Social network to fetch feed from",
      "type": "string"
    },
    "filter": {
      "description": "What to fetch",
      "type": "object",
      "required": [ "account" ],
      "properties": {
        "account": { "description": "Register a batch of accounts", "type": "array", "items": { "type": "string" } }
      }
    }
  }
}
